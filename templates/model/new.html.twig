{% extends 'base_admin_back.html.twig' %}

{% block title %}New Model{% endblock %}

{% block body %}
{{ form_start(form) }}
<div class="form-group row">
    <label class="col-lg-4 col-form-label">{{ form_label(form.model) }} <span class="text-danger">*</span></label>
    <div class="col-lg-6">
        {{ form_widget(form.model, {'attr': {'class': 'form-control'}}) }}
        {{ form_errors(form.model) }}
    </div>
</div>
<div class="form-group row">
    <div class="col-lg-8 ml-auto">
        <button type="submit" class="btn btn-primary">{{ button_label|default('Save') }}</button>
    </div>
</div>
{{ form_end(form) }}
<a href="{{ path('app_model_index') }}" class="btn btn-warning">back to list</a>
{% endblock %}
